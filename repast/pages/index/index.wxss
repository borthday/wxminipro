.home-wrap{
  background: #f4f4f4;
  padding: 30rpx;
  box-sizing: border-box;
}
.big-img-wrap image{
  width: 100%;
  height: 100%;
  display: block;
}
.big-img-wrap{
  position: relative;
  height: 100%;
}
.btn{
  width: 400rpx;
  height: 80rpx;
  background: white;
  border: 1px solid #1afa29;
  color: #1afa29;
  text-align: center;
  line-height: 80rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.order-phone{
  top: 100rpx;
  left: 50%;
  margin-left: -200rpx;
}
.menu{
  top: 200rpx;
  left: 50%;
