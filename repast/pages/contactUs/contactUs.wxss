/* pages/contactUs/contactUs.wxss */
.contact-wrap{
  padding: 30rpx;
  background: #f4f4f4;
  box-sizing: border-box;
}
.contact-content image{
  width:100%;
  height: 350rpx;
  display: block;
}
.shop-name{
  text-align: center;
  margin-top: 40rpx;
  margin-bottom: 60rpx;
  font-size: 48rpx;
}
.contact-phone,.contact-add,.contact-mail{
  text-align: center;
  margin-bottom: 30rpx;
  font-size: 24rpx;
}
.contact-phone .title,.contact-add .title,.contact-mail .title{
  text-align: center;
}